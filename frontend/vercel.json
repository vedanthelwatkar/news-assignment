{
  "version": 2,
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/browser/assets/$1"
    },
    {
      "src": "/(.*).(js|css|ico|json)",
      "dest": "/browser/$1.$2"
    },
    {
      "src": "/(.*)",
      "dest": "/browser/index.html"
    }
  ]
}
